extends layout 


block main-content 
  //-You can overwrite the main-content block by writing block content
  //-in the template file which is extending.
  h1.page-title Hello! 
  ul 
    each m in movies 
      li 
        h2 
          a(href=`/movie/${m.movie.ids.imdb}`) #{m.movie.title} (#{m.movie.year})
        p #{m.movie.tagline}

  